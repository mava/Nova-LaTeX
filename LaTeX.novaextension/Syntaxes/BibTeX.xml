<?xml version="1.0" encoding="UTF-8"?>
<syntax name="bibtex">
    <meta>
        <name>BibTeX</name>
        <type>structured</type>
        <preferred-file-extension>bib</preferred-file-extension>
    </meta>
    
    <detectors>
        <extension priority="1.0">bib</extension>
    </detectors>
    
    <tree-sitter>
        <highlights path="bibtex/highlights.scm" />
        <folds path="bibtex/folds.scm" />
        <symbols path="bibtex/symbols.scm" />
        <text-checking path="bibtex/textChecking.scm" />
    </tree-sitter>
</syntax>
